{
    "name": "@danny270793/azureservicesclient",
    "version": "1.0.0",
    "description": "Library to interact con azure services",
    "main": "./build/index.js",
    "types": "build/index.d.ts",
    "files": [
        "build/**/*"
    ],
    "scripts": {
        "format": "prettier ./ --write",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "build": "tsc",
        "build:force": "rm -r -f ./build && tsc",
        "start": "node ./build/index.js",
        "start:watch": "ts-node-dev --respawn ./src/index.ts",
        "test": "node --test ./build/tests",
        "test:watch": "ts-node-dev --respawn ./tests/index.test.ts"
    },
    "keywords": [
        "levenshtein"
    ],
    "author": {
        "name": "Danny Vaca",
        "email": "danny270793@gmail.com",
        "url": "https://danny270793.github.io/#/"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/danny270793/AzureServicesClient.git"
    },
    "license": "MIT",
    "devDependencies": {
        "@eslint/js": "^9.9.0",
        "@types/node": "^22.2.0",
        "eslint": "^9.9.0",
        "globals": "^15.9.0",
        "prettier": "^3.3.3",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.5.4",
        "typescript-eslint": "^8.0.1"
    },
    "dependencies": {
        "axios": "^1.7.3"
    }
}
